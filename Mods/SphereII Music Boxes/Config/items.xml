<configs>

  <append xpath="/items">
    
    <!-- Audio CD examples. It references a SoundDataNode in sounds.xml -->
		<item name="audioCD00">
		  <property name="Extends" value="casinoCoin"/>
		  <property name="Meshfile" value="#@modfolder:Resources/MediaCases.unity3d?CDCase00_X" /> 
		  <property name="HandMeshfile" value="#@modfolder:Resources/MediaCases.unity3d?CDCase00_X"/>
		  <property name="HoldType" value="21"/> 
		  <property name="CustomIcon" value="CD" />
		  <property name="Material" value="Mplastics"/>
		  <property name="Stacknumber" value="1"/>
		  <property name="SoundDataNode" value="christmasmusic01" />
		</item>
		<item name="audioCD01">
		  <property name="Extends" value="casinoCoin"/>
		  <property name="Meshfile" value="#@modfolder:Resources/MediaCases.unity3d?xmascd" /> 
